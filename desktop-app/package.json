{
  "name": "diddl-manager",
  "version": "1.13.0",
  "description": "App to manage your Diddls",
  "main": "./out/main/index.js",
  "author": "Brandontam29",
  "homepage": "https://electron-vite.org",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "oxlint --type-aware ./src/",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "bun run typecheck:node && bun run typecheck:web",
    "preview": "electron-vite preview",
    "start": "electron-vite dev --watch",
    "dev": "electron-vite dev --watch",
    "build": "electron-vite build",
    "build-local:win": "electron-vite build && electron-builder --win",
    "build-local:mac": "electron-vite build && electron-builder --mac",
    "build-local:linux": "electron-vite build && electron-builder --linux",
    "build:win": "bun run build && electron-builder --win --publish always",
    "build:mac": "electron-vite build && electron-builder --mac --publish always",
    "build:linux": "electron-vite build && electron-builder --linux --publish always",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@kobalte/core": "^0.13.8",
    "@solid-primitives/intersection-observer": "^2.2.2",
    "@solid-primitives/media": "^2.3.3",
    "@solidjs/router": "^0.15.4",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/solid-form": "^1.27.6",
    "adm-zip": "^0.5.16",
    "better-sqlite3": "^12.4.5",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-updater": "^6.3.9",
    "kysely": "^0.28.8",
    "kysely-plugin-serialize": "^0.8.2",
    "lucide-solid": "^0.441.0",
    "motion": "^12.23.26",
    "solid-icons": "^1.1.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/adm-zip": "^0.5.7",
    "@types/better-sqlite3": "^7.6.13",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^22.16.5",
    "autoprefixer": "^10.4.21",
    "electron": "^37.2.3",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-oxlint": "^1.35.0",
    "eslint-plugin-solid": "^0.14.5",
    "kysely-codegen": "^0.19.0",
    "oxlint": "^1.35.0",
    "oxlint-tsgolint": "^0.10.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "solid-js": "^1.9.10",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^6.3.5",
    "vite-plugin-solid": "^2.11.7",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
